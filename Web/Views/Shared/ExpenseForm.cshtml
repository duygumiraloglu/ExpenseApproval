
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <title>Masraf Tanımlama, Düzenleme ve Listeleme Ekranı</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>    
  
</head>
<body>

    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <div class="navbar-brand" style="animation:ease-in">Masraf Tanımlama Portalına Hoş Geldiniz</div>
            </div>
            <ul class="nav navbar-nav">
                <li class="active"><a href="/ExpenseForm/Index">Masraf Tanımla</a></li>
                <li><a href="#">Onay Bekleyenler</a></li>
                <li><a href="#">Masraf Listesi</a></li>
            </ul>
            <form method="post" asp-action="SignOut" asp-controller="SignOut" class="navbar-form navbar-right">
                @Html.AntiForgeryToken()
                <button type="submit" class="btn btn-danger">Çıkış Yap</button>
            </form>
           
        </div>
    </nav>

    <div class="container">
        @RenderBody()
    </div>

</body>

@RenderSection("scripts", required: false)
</html>


